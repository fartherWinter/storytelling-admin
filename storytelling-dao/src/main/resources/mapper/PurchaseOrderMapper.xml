<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chennian.storytelling.dao.PurchaseOrderMapper">

    <!-- PurchaseOrder结果映射 -->
    <resultMap id="PurchaseOrderResult" type="com.chennian.storytelling.bean.model.PurchaseOrder">
        <id property="orderId" column="order_id" />
